timeout 10 'brightnessctl -c backlight set 50%-' resume 'brightnessctl -c backlight set +50%'
timeout 20 'playerctl pause'
timeout 30 'hyprctl dispatch dpms off' resume 'sleep 1; hyprctl dispatch dpms on'
timeout 40 'swaylock'
timeout 50 'systemctl suspend'
before-sleep 'swaylock'
after-resume 'hyprctl dispatch dpms on'
lock 'swaylock'
